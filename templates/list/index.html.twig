{% extends 'base.html.twig' %}

{% block title %}The Joe Rogan Catalog!{% endblock %}

{% block body %}
<style>
    .wrapper { margin: 1em auto; max-width: 800px; width: 95%; font: 18px/1.5 sans-serif; }
</style>

<div class="wrapper">
<ul>
  {% for item in list %}
    <li><a href="https://youtu.be/{{ item.videoId }}" target="_blank">{{ item.title }}</a></li>
  {% endfor %}
</ul>
</div>
{% endblock %}
